<odoo>
    <record id='custom_list' model='ir.ui.view'>
        <field name='name'>inventory.module.tree.custom</field>
        <field name='model'>inventory.module</field>
        <field name="type">list</field>
        <field name='arch' type='xml'>
            <list>
                <field name='reference'/>
                <field name='contact'/>
                <field name='schedule_date'/>
                <field name='source_doc'/>
                <field name='company'/>
                <field name='status'/>
            </list>
        </field>
    </record>

    <record id='custom_form' model='ir.ui.view'>
        <field name='name'>inventory.module.form.custom</field>
        <field name='model'>inventory.module</field>
        <field name='arch' type='xml'>
            <form>
                <group>
                    <field name='sender'/>
                    <field name='operation_type'/>
                    <field name='schedule_date'/>
                    <field name='source_doc'/>
                    <field name='company'/>
                    <field name='status'/>
                </group>
            </form>
        </field>
    </record>
</odoo>