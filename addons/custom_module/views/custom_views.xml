<odoo>
    <record id='first_order_tree_custom' model='ir.ui.view'>
        <field name='name'>first.order.tree.custom</field>
        <field name='model'>custom.module</field>
        <field name='arch' type='xml'>
            <tree string='Sales Orders'>
                <field name='name1'/>
                <field name='name2'/>
                <field name='name3'/>
                <field name='name4'/>
                <field name='name5'/>
            </tree>
        </field>
    </record>

    <record id='first_order_from_custom' model='ir.ui.view'>
        <field name='name'>first.order.tree.custom</field>
        <field name='model'>custom.module</field>
        <field name='arch' type='xml'>
            <form string='Sales Orders' sample='1'>
                <group>
                    <field name='name1'/>
                    <field name='name2'/>
                    <field name='name3'/>
                    <field name='name4'/>
                    <field name='name5'/>
                </group>
            </form>
        </field>
    </record>

    <record id='action_custom_model' model='ir.actions.act_window'>
        <field name='name'>Custom Module</field>
        <field name='res_model'>custom.module</field>
        <field name='view_mode'>tree,form</field>
        <field name='target'>current</field>
        <field name='help' type='html'>
            <p>Create and manage your custom models here.</p>
        </field>
    </record>

    <menuitem id='menu_custom_module_root_menu' name='Custom Module'/>
    <menuitem id='menu_custom_module_sub_root_menu' name='Name list' parent='menu_custom_module_root_menu'/>
    <menuitem id='menu_custom_module_menu' name='List' parent='menu_custom_module_sub_root_menu'/>
</odoo>